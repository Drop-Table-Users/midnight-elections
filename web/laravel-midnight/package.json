{
  "name": "@versiontwo/midnight-laravel",
  "version": "1.0.0",
  "description": "Laravel integration package for the Midnight blockchain network",
  "type": "module",
  "main": "./dist/midnight.umd.js",
  "module": "./dist/midnight.es.js",
  "types": "./dist/types/app.d.ts",
  "exports": {
    ".": {
      "import": "./dist/midnight.es.js",
      "require": "./dist/midnight.umd.js",
      "types": "./dist/types/app.d.ts"
    },
    "./dist/midnight.css": "./dist/midnight.css"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "laravel",
    "midnight",
    "blockchain",
    "zero-knowledge",
    "zk-proofs",
    "privacy",
    "web3",
    "wallet"
  ],
  "author": "VersionTwo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/versiontwo/midnight-laravel"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly --declaration --declarationDir dist/types",
    "build:watch": "vite build --watch",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint resources/js --ext .ts,.vue",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "alpinejs": "^3.13.0",
    "axios": "^1.6.0",
    "typescript": "^5.3.0",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitest/ui": "^4.0.10",
    "@vue/test-utils": "^2.4.0",
    "autoprefixer": "^10.4.0",
    "happy-dom": "^20.0.10",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "terser": "^5.26.0",
    "vite": "^7.2.2",
    "vitest": "^4.0.10"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
