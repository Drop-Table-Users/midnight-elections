{
    "name": "versiontwo/midnight-laravel",
    "description": "Laravel integration package for the Midnight blockchain network",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "midnight",
        "blockchain",
        "zero-knowledge",
        "zk-proofs",
        "privacy"
    ],
    "authors": [
        {
            "name": "VersionTwo",
            "email": "info@versiontwo.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "laravel/framework": "^12.0",
        "guzzlehttp/guzzle": "^7.8",
        "illuminate/support": "^12.0",
        "illuminate/contracts": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^10.0",
        "mockery/mockery": "^1.6",
        "phpstan/phpstan": "^1.10",
        "laravel/pint": "^1.13"
    },
    "autoload": {
        "psr-4": {
            "VersionTwo\\Midnight\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VersionTwo\\Midnight\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VersionTwo\\Midnight\\Providers\\MidnightServiceProvider"
            ],
            "aliases": {
                "Midnight": "VersionTwo\\Midnight\\Facades\\Midnight",
                "MidnightContracts": "VersionTwo\\Midnight\\Facades\\MidnightContracts",
                "MidnightWallet": "VersionTwo\\Midnight\\Facades\\MidnightWallet"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "test": "phpunit",
        "analyse": "phpstan analyse",
        "format": "pint"
    }
}
